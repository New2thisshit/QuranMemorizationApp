  "data": [
    {
      "number": 1,
      "name": "سُورَةُ ٱلْفَاتِحَةِ",
      "englishName": "Al-Fatiha",
      "englishNameTranslation": "The Opening",
      "numberOfAyahs": 7,
      "revelationType": "Meccan",
      "ayahs": [
        {
          "number": 1,
          "numberInSurah": 1,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
          "translation": "In the name of Allah, the Entirely Merciful, the Especially Merciful",
          "transliteration": "Bismi Allahi alrrahmani alrraheemi"
        },
        {
          "number": 2,
          "numberInSurah": 2,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
          "translation": "All praise is due to Allah, Lord of the worlds",
          "transliteration": "Alhamdu lillahi rabbi alAAalameena"
        },
        {
          "number": 3,
          "numberInSurah": 3,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "الرَّحْمَٰنِ الرَّحِيمِ",
          "translation": "The Entirely Merciful, the Especially Merciful",
          "transliteration": "Alrrahmani alrraheemi"
        },
        {
          "number": 4,
          "numberInSurah": 4,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "مَالِكِ يَوْمِ الدِّينِ",
          "translation": "Sovereign of the Day of Recompense",
          "transliteration": "Maliki yawmi alddeeni"
        },
        {
          "number": 5,
          "numberInSurah": 5,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
          "translation": "It is You we worship and You we ask for help",
          "transliteration": "Iyyaka naAAbudu waiyyaka nastaAAeenu"
        },
        {
          "number": 6,
          "numberInSurah": 6,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
          "translation": "Guide us to the straight path",
          "transliteration": "Ihdina alssirata almustaqeema"
        },
        {
          "number": 7,
          "numberInSurah": 7,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
          "translation": "The path of those upon whom You have bestowed favor, not of those who have earned Your anger or of those who are astray",
          "transliteration": "Sirata allatheena anAAamta AAalayhim ghayri almaghdoobi AAalayhim wala alddalleena"
        }
      ]
    },
    {
      "number": 2,
      "name": "سُورَةُ البَقَرَةِ",
      "englishName": "Al-Baqarah",
      "englishNameTranslation": "The Cow",
      "numberOfAyahs": 286,
      "revelationType": "Medinan",
      "ayahs": [
        {
          "number": 8,
          "numberInSurah": 1,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "الم",
          "translation": "Alif, Lam, Meem",
          "transliteration": "Alif-lam-meem"
        },
        {
          "number": 9,
          "numberInSurah": 2,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "ذَٰلِكَ الْكِتَابُ لَا رَيْبَ ۛ فِيهِ ۛ هُدًى لِّلْمُتَّقِينَ",
          "translation": "This is the Book about which there is no doubt, a guidance for those conscious of Allah",
          "transliteration": "Thalika alkitabu la rayba feehi hudan lilmuttaqeena"
        },
        {
          "number": 10,
          "numberInSurah": 3,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "الَّذِينَ يُؤْمِنُونَ بِالْغَيْبِ وَيُقِيمُونَ الصَّلَاةَ وَمِمَّا رَزَقْنَاهُمْ يُنفِقُونَ",
          "translation": "Who believe in the unseen, establish prayer, and spend out of what We have provided for them",
          "transliteration": "Allatheena yuminoona bilghaybi wayuqeemoona alssalata wamimma razaqnahum yunfiqoona"
        },
        {
          "number": 11,
          "numberInSurah": 4,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "وَالَّذِينَ يُؤْمِنُونَ بِمَا أُنزِلَ إِلَيْكَ وَمَا أُنزِلَ مِن قَبْلِكَ وَبِالْآخِرَةِ هُمْ يُوقِنُونَ",
          "translation": "And who believe in what has been revealed to you, and what was revealed before you, and of the Hereafter they are certain",
          "transliteration": "Waallatheena yuminoona bima onzila ilayka wama onzila min qablika wabialakhirati hum yooqinoona"
        },
        {
          "number": 12,
          "numberInSurah": 5,
          "juz": 1,
          "manzil": 1,
          "ruku": 1,
          "hizbQuarter": 1,
          "sajda": false,
          "text": "أُولَٰئِكَ عَلَىٰ هُدًى مِّن رَّبِّهِمْ ۖ وَأُولَٰئِكَ هُمُ الْمُفْلِحُونَ",
          "translation": "Those are upon guidance from their Lord, and it is those who are the successful",
          "transliteration": "Ola-ika AAala hudan min rabbihim waola-ika humu almuflihoona"
        }
      ]
    },
    {
